<Window x:Class="APKRepackageSDKTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:APKRepackageSDKTool"
        mc:Ignorable="d"
        Title="APK重打包工具" Height="407.859" Width="684.35">
    <Grid>
        <Grid Margin="10,10,0,10" HorizontalAlignment="Left" Width="265">
            <TextBox x:Name="Text_APKPath" Margin="10,40,0,0" TextWrapping="Wrap" RenderTransformOrigin="0.673,4.796" MaxLines="1" HorizontalAlignment="Left" Width="137" Height="23" VerticalAlignment="Top"/>
            <Label Content="APK目录" Margin="10,10,0,0" HorizontalAlignment="Left" Width="58" Height="25" VerticalAlignment="Top"/>
            <Button Content="浏览" Margin="161,44,0,0" Click="Button_ClickSelectAPK" HorizontalAlignment="Left" Width="75" Height="19" VerticalAlignment="Top"/>
        </Grid>
        <Button Content="重打包" Margin="0,0,10,10" Height="30" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="Button_ClicRepackageAPK"/>
        <TextBox x:Name="Text_output" HorizontalAlignment="Right" Margin="0,58,10,53" TextWrapping="Wrap" Width="347"/>
        <Label Content="输出" Margin="0,20,302,0" RenderTransformOrigin="0.021,0.189" HorizontalAlignment="Right" Width="55" Height="25" VerticalAlignment="Top"/>

    </Grid>
</Window>
